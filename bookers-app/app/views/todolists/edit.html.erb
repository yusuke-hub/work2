<div class="edit">
<h1>Editing Book</h1>
<%= form_for(@list, url:"/todolists") do |f| %>
<h4>Title</h4>
<%= f.text_field :title %>
<h4>Body</h4>
<%= f.text_area :body %><br><br>
<%= f.submit "Update Book" %>
<p><%= link_to "Edit",edit_todolist_path(@list.id) %> | <%= link_to "Back", index_todolist_path %></p>
<% end %>
</div>